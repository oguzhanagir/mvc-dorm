@model EntityLayer.Concrete.SocialMedia
@{
    ViewBag.Title = "SocialMediaNew";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<form class="form-group" method="post">

    @Html.Label("Sosyal Medya Adı")
    @Html.TextBoxFor(x => x.SocialMediaName, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya Linki")
    @Html.TextBoxFor(x => x.SocialMediaLink, new { @class = "form-control" })


    <button class="btn btn-rounded btn-success mt-3"> Kaydet</button>
</form>


